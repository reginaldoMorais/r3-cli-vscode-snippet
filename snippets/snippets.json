{
  
	"Console log": {
		"prefix": "r3-clg",
		"body": [
			"console.log('${1:label}', ${2:object});"
		],
		"description": "Create Console log with label"
	},

	"Console error": {
		"prefix": "r3-cer",
		"body": [
			"console.error(${1:object});"
		],
		"description": "Create Console error"
	},

	"Console warn": {
		"prefix": "r3-cwa",
